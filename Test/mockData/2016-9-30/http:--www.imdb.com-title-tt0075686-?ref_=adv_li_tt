<script>
    setTimeout(function(){
        try{
            //sis3.0 pixel
            var url_sis3 = 'http://s.amazon-adsystem.com/iu3?',
                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=0101d7bf04cece6db4e2f37bd50b87165c758317d726fd91872bbb1b8b7b0fcef26e",
                    "pId=tt0075686",
                    "r=1",
