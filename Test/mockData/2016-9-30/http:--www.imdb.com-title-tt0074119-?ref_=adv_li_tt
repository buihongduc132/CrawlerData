                    <button class="btn small" value="Yes" name="ynb_145677_yes" onclick="CS.TMD.user_review_vote(145677, 'tt0074119', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_145677_no" onclick="CS.TMD.user_review_vote(145677, 'tt0074119', 'no');" >No</button>
                </div>
