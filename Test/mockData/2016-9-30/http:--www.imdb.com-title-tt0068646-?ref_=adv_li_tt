                    <button class="btn small" value="Yes" name="ynb_1066533_yes" onclick="CS.TMD.user_review_vote(1066533, 'tt0068646', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1066533_no" onclick="CS.TMD.user_review_vote(1066533, 'tt0068646', 'no');" >No</button>
