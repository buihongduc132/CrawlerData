                    <button class="btn small" value="Yes" name="ynb_21823_yes" onclick="CS.TMD.user_review_vote(21823, 'tt0032553', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_21823_no" onclick="CS.TMD.user_review_vote(21823, 'tt0032553', 'no');" >No</button>
                </div>
