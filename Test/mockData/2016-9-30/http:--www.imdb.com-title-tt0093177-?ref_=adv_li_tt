                    <button class="btn small" value="Yes" name="ynb_245145_yes" onclick="CS.TMD.user_review_vote(245145, 'tt0093177', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_245145_no" onclick="CS.TMD.user_review_vote(245145, 'tt0093177', 'no');" >No</button>
                </div>
