                    <button class="btn small" value="Yes" name="ynb_1374929_yes" onclick="CS.TMD.user_review_vote(1374929, 'tt0285531', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1374929_no" onclick="CS.TMD.user_review_vote(1374929, 'tt0285531', 'no');" >No</button>
                </div>
