    setTimeout(function(){
        try{
            //sis3.0 pixel
            var url_sis3 = 'http://s.amazon-adsystem.com/iu3?',
                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=010144f0e37001e9c561647b472c336f1c942c94518122288c99025ad1f6b2637150",
                    "pId=tt0158983",
                    "r=1",
                    "rP=http%3A%2F%2Fwww.imdb.com%2Ftitle%2Ftt0158983%2F%3Fref_%3Dadv_li_tt",
                    "ex-hargs=v=1.0;c=IMDB;p=tt0158983;t=imdb_title_view;",
                    "encoding=server",
                    "cb=386404227421"  
                ];
        
            if (document.getElementById('sis_pixel_sitewide')) {
                (document.getElementById('sis_pixel_sitewide')).src = url_sis3 + params_sis3.join('&');
            }
        }
        catch(e){
            if ('consoleLog' in window){
                consoleLog('Pixel failure ' + e.toString(),'sis');
            }
            if (window.ueLogError) { 
                window.ueLogError(e);
            }
        }
    }, 20);
</script>
