                    <button class="btn small" value="Yes" name="ynb_1733335_yes" onclick="CS.TMD.user_review_vote(1733335, 'tt0087050', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1733335_no" onclick="CS.TMD.user_review_vote(1733335, 'tt0087050', 'no');" >No</button>
                </div>
