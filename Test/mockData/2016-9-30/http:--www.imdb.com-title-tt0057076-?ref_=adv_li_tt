                    <button class="btn small" value="Yes" name="ynb_81645_yes" onclick="CS.TMD.user_review_vote(81645, 'tt0057076', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_81645_no" onclick="CS.TMD.user_review_vote(81645, 'tt0057076', 'no');" >No</button>
                </div>
