<script>
    setTimeout(function(){
        try{
            //sis3.0 pixel
            var url_sis3 = 'http://s.amazon-adsystem.com/iu3?',
                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=0101393cab3e96654f42b5f05a45e0ae43bb8f79ce6896df14ef8f83f098e886dd06",
                    "pId=tt0109424",
                    "r=1",
                    "rP=http%3A%2F%2Fwww.imdb.com%2Ftitle%2Ftt0109424%2F%3Fref_%3Dadv_li_tt",
                    "ex-hargs=v=1.0;c=IMDB;p=tt0109424;t=imdb_title_view;",
                    "encoding=server",
                    "cb=369619002213"  
                ];
        
            if (document.getElementById('sis_pixel_sitewide')) {
                (document.getElementById('sis_pixel_sitewide')).src = url_sis3 + params_sis3.join('&');
            }
        }
        catch(e){
            if ('consoleLog' in window){
                consoleLog('Pixel failure ' + e.toString(),'sis');
            }
            if (window.ueLogError) { 
                window.ueLogError(e);
            }
        }
    }, 20);
</script>
