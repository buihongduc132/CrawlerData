                    <button class="btn small" value="Yes" name="ynb_1359960_yes" onclick="CS.TMD.user_review_vote(1359960, 'tt0105151', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1359960_no" onclick="CS.TMD.user_review_vote(1359960, 'tt0105151', 'no');" >No</button>
                </div>
