                    <button class="btn small" value="Yes" name="ynb_1329724_yes" onclick="CS.TMD.user_review_vote(1329724, 'tt0022100', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1329724_no" onclick="CS.TMD.user_review_vote(1329724, 'tt0022100', 'no');" >No</button>
                </div>
