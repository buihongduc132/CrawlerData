                    <button class="btn small" value="Yes" name="ynb_269654_yes" onclick="CS.TMD.user_review_vote(269654, 'tt0097351', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_269654_no" onclick="CS.TMD.user_review_vote(269654, 'tt0097351', 'no');" >No</button>
                </div>
