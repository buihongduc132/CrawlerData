                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=010171e999925a58af0db5bf6f3e4f04026658e99e712dbbf8fbd8163e4b69ca9377",
                    "pId=tt0107050",
                    "r=1",
