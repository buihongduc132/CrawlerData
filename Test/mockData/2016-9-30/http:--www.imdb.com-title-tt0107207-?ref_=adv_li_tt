                    <button class="btn small" value="Yes" name="ynb_989655_yes" onclick="CS.TMD.user_review_vote(989655, 'tt0107207', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_989655_no" onclick="CS.TMD.user_review_vote(989655, 'tt0107207', 'no');" >No</button>
                </div>
