                    <button class="btn small" value="Yes" name="ynb_1711027_yes" onclick="CS.TMD.user_review_vote(1711027, 'tt0486655', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1711027_no" onclick="CS.TMD.user_review_vote(1711027, 'tt0486655', 'no');" >No</button>
                </div>
