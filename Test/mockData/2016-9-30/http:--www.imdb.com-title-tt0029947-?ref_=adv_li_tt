                    <button class="btn small" value="Yes" name="ynb_980397_yes" onclick="CS.TMD.user_review_vote(980397, 'tt0029947', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_980397_no" onclick="CS.TMD.user_review_vote(980397, 'tt0029947', 'no');" >No</button>
                </div>
