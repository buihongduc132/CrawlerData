                    <button class="btn small" value="Yes" name="ynb_1195058_yes" onclick="CS.TMD.user_review_vote(1195058, 'tt0044081', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1195058_no" onclick="CS.TMD.user_review_vote(1195058, 'tt0044081', 'no');" >No</button>
                </div>
            <div class="see-more">
                
