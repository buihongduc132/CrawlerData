                    <button class="btn small" value="Yes" name="ynb_230899_yes" onclick="CS.TMD.user_review_vote(230899, 'tt0090756', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_230899_no" onclick="CS.TMD.user_review_vote(230899, 'tt0090756', 'no');" >No</button>
                </div>
