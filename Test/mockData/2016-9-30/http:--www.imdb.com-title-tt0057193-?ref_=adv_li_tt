                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=01013ff68a42e40c1ea4e722c7a619681ca1af58e006838e1405684bb3383ba4001f",
                    "pId=tt0057193",
                    "r=1",
