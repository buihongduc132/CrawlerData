                    <button class="btn small" value="Yes" name="ynb_1244914_yes" onclick="CS.TMD.user_review_vote(1244914, 'tt0249462', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1244914_no" onclick="CS.TMD.user_review_vote(1244914, 'tt0249462', 'no');" >No</button>
                </div>
