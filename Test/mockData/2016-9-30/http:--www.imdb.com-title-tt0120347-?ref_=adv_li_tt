                    <button class="btn small" value="Yes" name="ynb_438017_yes" onclick="CS.TMD.user_review_vote(438017, 'tt0120347', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_438017_no" onclick="CS.TMD.user_review_vote(438017, 'tt0120347', 'no');" >No</button>
                </div>
