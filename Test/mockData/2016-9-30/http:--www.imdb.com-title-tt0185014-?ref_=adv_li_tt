                    <button class="btn small" value="Yes" name="ynb_623431_yes" onclick="CS.TMD.user_review_vote(623431, 'tt0185014', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_623431_no" onclick="CS.TMD.user_review_vote(623431, 'tt0185014', 'no');" >No</button>
                </div>
