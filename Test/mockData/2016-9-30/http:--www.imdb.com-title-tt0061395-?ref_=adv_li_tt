                    <button class="btn small" value="Yes" name="ynb_1047025_yes" onclick="CS.TMD.user_review_vote(1047025, 'tt0061395', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1047025_no" onclick="CS.TMD.user_review_vote(1047025, 'tt0061395', 'no');" >No</button>
                </div>
