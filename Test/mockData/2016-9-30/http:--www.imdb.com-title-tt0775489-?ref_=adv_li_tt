                    <button class="btn small" value="Yes" name="ynb_2327341_yes" onclick="CS.TMD.user_review_vote(2327341, 'tt0775489', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2327341_no" onclick="CS.TMD.user_review_vote(2327341, 'tt0775489', 'no');" >No</button>
                </div>
