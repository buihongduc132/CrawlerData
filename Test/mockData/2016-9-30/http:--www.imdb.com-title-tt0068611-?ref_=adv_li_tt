    setTimeout(function(){
        try{
            //sis3.0 pixel
            var url_sis3 = 'http://s.amazon-adsystem.com/iu3?',
                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=010145883bd37a38ca4b10e2f7f9ecb4b05476010e45fa4d6bb8754291c51aa29227",
                    "pId=tt0068611",
                    "r=1",
