                    <button class="btn small" value="Yes" name="ynb_1082407_yes" onclick="CS.TMD.user_review_vote(1082407, 'tt0363226', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1082407_no" onclick="CS.TMD.user_review_vote(1082407, 'tt0363226', 'no');" >No</button>
                </div>
