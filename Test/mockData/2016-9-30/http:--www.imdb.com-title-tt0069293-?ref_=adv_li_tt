                    <button class="btn small" value="Yes" name="ynb_1188724_yes" onclick="CS.TMD.user_review_vote(1188724, 'tt0069293', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1188724_no" onclick="CS.TMD.user_review_vote(1188724, 'tt0069293', 'no');" >No</button>
                </div>
