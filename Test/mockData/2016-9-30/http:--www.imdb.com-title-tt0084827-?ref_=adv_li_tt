                    <button class="btn small" value="Yes" name="ynb_198012_yes" onclick="CS.TMD.user_review_vote(198012, 'tt0084827', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_198012_no" onclick="CS.TMD.user_review_vote(198012, 'tt0084827', 'no');" >No</button>
                </div>
