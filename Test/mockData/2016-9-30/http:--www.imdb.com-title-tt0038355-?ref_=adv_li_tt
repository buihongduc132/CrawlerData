                    <button class="btn small" value="Yes" name="ynb_1790111_yes" onclick="CS.TMD.user_review_vote(1790111, 'tt0038355', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1790111_no" onclick="CS.TMD.user_review_vote(1790111, 'tt0038355', 'no');" >No</button>
                </div>
