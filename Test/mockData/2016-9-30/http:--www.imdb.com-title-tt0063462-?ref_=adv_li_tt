                    <button class="btn small" value="Yes" name="ynb_1577841_yes" onclick="CS.TMD.user_review_vote(1577841, 'tt0063462', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1577841_no" onclick="CS.TMD.user_review_vote(1577841, 'tt0063462', 'no');" >No</button>
                </div>
