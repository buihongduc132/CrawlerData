                    <button class="btn small" value="Yes" name="ynb_983975_yes" onclick="CS.TMD.user_review_vote(983975, 'tt0309987', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_983975_no" onclick="CS.TMD.user_review_vote(983975, 'tt0309987', 'no');" >No</button>
                </div>
