                    <button class="btn small" value="Yes" name="ynb_42139_yes" onclick="CS.TMD.user_review_vote(42139, 'tt0042332', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_42139_no" onclick="CS.TMD.user_review_vote(42139, 'tt0042332', 'no');" >No</button>
                </div>
