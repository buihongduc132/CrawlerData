                    <button class="btn small" value="Yes" name="ynb_822810_yes" onclick="CS.TMD.user_review_vote(822810, 'tt0289765', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_822810_no" onclick="CS.TMD.user_review_vote(822810, 'tt0289765', 'no');" >No</button>
                </div>
