                    <button class="btn small" value="Yes" name="ynb_53207_yes" onclick="CS.TMD.user_review_vote(53207, 'tt0047478', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_53207_no" onclick="CS.TMD.user_review_vote(53207, 'tt0047478', 'no');" >No</button>
                </div>
