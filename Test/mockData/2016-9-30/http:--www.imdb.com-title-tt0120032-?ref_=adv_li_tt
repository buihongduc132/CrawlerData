                    <button class="btn small" value="Yes" name="ynb_1925595_yes" onclick="CS.TMD.user_review_vote(1925595, 'tt0120032', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1925595_no" onclick="CS.TMD.user_review_vote(1925595, 'tt0120032', 'no');" >No</button>
                </div>
