                    <button class="btn small" value="Yes" name="ynb_268200_yes" onclick="CS.TMD.user_review_vote(268200, 'tt0097123', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_268200_no" onclick="CS.TMD.user_review_vote(268200, 'tt0097123', 'no');" >No</button>
                </div>
