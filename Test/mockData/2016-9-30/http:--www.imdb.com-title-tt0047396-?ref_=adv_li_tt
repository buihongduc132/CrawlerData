                    <button class="btn small" value="Yes" name="ynb_52655_yes" onclick="CS.TMD.user_review_vote(52655, 'tt0047396', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_52655_no" onclick="CS.TMD.user_review_vote(52655, 'tt0047396', 'no');" >No</button>
