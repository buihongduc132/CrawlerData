                    <button class="btn small" value="Yes" name="ynb_1347473_yes" onclick="CS.TMD.user_review_vote(1347473, 'tt0384537', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1347473_no" onclick="CS.TMD.user_review_vote(1347473, 'tt0384537', 'no');" >No</button>
