                    <button class="btn small" value="Yes" name="ynb_330824_yes" onclick="CS.TMD.user_review_vote(330824, 'tt0108358', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_330824_no" onclick="CS.TMD.user_review_vote(330824, 'tt0108358', 'no');" >No</button>
                </div>
