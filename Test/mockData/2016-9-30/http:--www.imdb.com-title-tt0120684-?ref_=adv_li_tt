                    <button class="btn small" value="Yes" name="ynb_453407_yes" onclick="CS.TMD.user_review_vote(453407, 'tt0120684', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_453407_no" onclick="CS.TMD.user_review_vote(453407, 'tt0120684', 'no');" >No</button>
                </div>
