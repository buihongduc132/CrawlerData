                    <button class="btn small" value="Yes" name="ynb_961388_yes" onclick="CS.TMD.user_review_vote(961388, 'tt0317705', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_961388_no" onclick="CS.TMD.user_review_vote(961388, 'tt0317705', 'no');" >No</button>
                </div>
