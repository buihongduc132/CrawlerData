                    <button class="btn small" value="Yes" name="ynb_1250485_yes" onclick="CS.TMD.user_review_vote(1250485, 'tt0408306', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1250485_no" onclick="CS.TMD.user_review_vote(1250485, 'tt0408306', 'no');" >No</button>
                </div>
