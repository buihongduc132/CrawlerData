                    <button class="btn small" value="Yes" name="ynb_1077296_yes" onclick="CS.TMD.user_review_vote(1077296, 'tt0310775', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1077296_no" onclick="CS.TMD.user_review_vote(1077296, 'tt0310775', 'no');" >No</button>
                </div>
