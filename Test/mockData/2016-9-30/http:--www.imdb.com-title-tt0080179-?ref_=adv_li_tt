    setTimeout(function(){
        try{
            //sis3.0 pixel
            var url_sis3 = 'http://s.amazon-adsystem.com/iu3?',
                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=0101ef1f43f6cba083a6358b1f646aeeed32b8552119f5a0e434c32fb7a3a6f7085f",
                    "pId=tt0080179",
                    "r=1",
                    "rP=http%3A%2F%2Fwww.imdb.com%2Ftitle%2Ftt0080179%2F%3Fref_%3Dadv_li_tt",
                    "ex-hargs=v=1.0;c=IMDB;p=tt0080179;t=imdb_title_view;",
                    "encoding=server",
                    "cb=221603240429"  
                ];
        
            if (document.getElementById('sis_pixel_sitewide')) {
                (document.getElementById('sis_pixel_sitewide')).src = url_sis3 + params_sis3.join('&');
            }
        }
        catch(e){
            if ('consoleLog' in window){
                consoleLog('Pixel failure ' + e.toString(),'sis');
            }
            if (window.ueLogError) { 
                window.ueLogError(e);
            }
        }
    }, 20);
</script>
