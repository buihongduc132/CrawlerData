<script>
    setTimeout(function(){
        try{
            //sis3.0 pixel
            var url_sis3 = 'http://s.amazon-adsystem.com/iu3?',
                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=010141721e77040d5a7a2a128e77340ad24eef0acdbfe33734940dfe3684ec5e8ab0",
                    "pId=tt0970416",
                    "r=1",
                    "rP=http%3A%2F%2Fwww.imdb.com%2Ftitle%2Ftt0970416%2F%3Fref_%3Dadv_li_tt",
                    "ex-hargs=v=1.0;c=IMDB;p=tt0970416;t=imdb_title_view;",
                    "encoding=server",
                    "cb=897320815918"  
                ];
        
            if (document.getElementById('sis_pixel_sitewide')) {
                (document.getElementById('sis_pixel_sitewide')).src = url_sis3 + params_sis3.join('&');
            }
        }
        catch(e){
            if ('consoleLog' in window){
                consoleLog('Pixel failure ' + e.toString(),'sis');
            }
            if (window.ueLogError) { 
                window.ueLogError(e);
            }
        }
    }, 20);
</script>
