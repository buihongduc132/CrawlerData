                    <button class="btn small" value="Yes" name="ynb_2425522_yes" onclick="CS.TMD.user_review_vote(2425522, 'tt1590089', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2425522_no" onclick="CS.TMD.user_review_vote(2425522, 'tt1590089', 'no');" >No</button>
                </div>
