                    <button class="btn small" value="Yes" name="ynb_3510681_yes" onclick="CS.TMD.user_review_vote(3510681, 'tt2277860', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_3510681_no" onclick="CS.TMD.user_review_vote(3510681, 'tt2277860', 'no');" >No</button>
                </div>
