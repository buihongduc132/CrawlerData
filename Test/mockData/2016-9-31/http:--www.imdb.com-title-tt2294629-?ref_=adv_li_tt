                    <button class="btn small" value="Yes" name="ynb_3002501_yes" onclick="CS.TMD.user_review_vote(3002501, 'tt2294629', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_3002501_no" onclick="CS.TMD.user_review_vote(3002501, 'tt2294629', 'no');" >No</button>
                </div>
