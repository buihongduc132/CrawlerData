                    <button class="btn small" value="Yes" name="ynb_3413972_yes" onclick="CS.TMD.user_review_vote(3413972, 'tt2379713', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_3413972_no" onclick="CS.TMD.user_review_vote(3413972, 'tt2379713', 'no');" >No</button>
                </div>
