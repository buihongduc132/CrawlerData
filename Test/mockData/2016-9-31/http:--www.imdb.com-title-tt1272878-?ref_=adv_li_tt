                    <button class="btn small" value="Yes" name="ynb_2884070_yes" onclick="CS.TMD.user_review_vote(2884070, 'tt1272878', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2884070_no" onclick="CS.TMD.user_review_vote(2884070, 'tt1272878', 'no');" >No</button>
                </div>
