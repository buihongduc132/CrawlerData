                    <button class="btn small" value="Yes" name="ynb_3521500_yes" onclick="CS.TMD.user_review_vote(3521500, 'tt4853102', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_3521500_no" onclick="CS.TMD.user_review_vote(3521500, 'tt4853102', 'no');" >No</button>
                </div>
