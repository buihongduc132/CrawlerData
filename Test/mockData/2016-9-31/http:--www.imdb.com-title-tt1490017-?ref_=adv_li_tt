                    <button class="btn small" value="Yes" name="ynb_2956032_yes" onclick="CS.TMD.user_review_vote(2956032, 'tt1490017', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2956032_no" onclick="CS.TMD.user_review_vote(2956032, 'tt1490017', 'no');" >No</button>
