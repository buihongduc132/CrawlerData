    setTimeout(function(){
        try{
            //sis3.0 pixel
            var url_sis3 = 'http://s.amazon-adsystem.com/iu3?',
                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=01019cdacbdb399f6a0f2bd46492c5e2ebd81c472ab1bb6a32a1df7613d3b968a0e1",
                    "pId=tt1340107",
                    "r=1",
                    "rP=http%3A%2F%2Fwww.imdb.com%2Ftitle%2Ftt1340107%2F%3Fref_%3Dadv_li_tt",
                    "ex-hargs=v=1.0;c=IMDB;p=tt1340107;t=imdb_title_view;",
                    "encoding=server",
                    "cb=678466043980"  
                ];
        
            if (document.getElementById('sis_pixel_sitewide')) {
                (document.getElementById('sis_pixel_sitewide')).src = url_sis3 + params_sis3.join('&');
            }
        }
        catch(e){
            if ('consoleLog' in window){
                consoleLog('Pixel failure ' + e.toString(),'sis');
            }
            if (window.ueLogError) { 
                window.ueLogError(e);
            }
        }
    }, 20);
</script>
