                params_sis3 = [
                    "d=imdb.com",
                    "a1=",
                    "a2=01015f2d4888fc15cba7e78cb29c04b3c62c400d7076a20c981ac2c77fbd7ba2587d",
                    "pId=tt1686821",
                    "r=1",
                    "rP=http%3A%2F%2Fwww.imdb.com%2Ftitle%2Ftt1686821%2F%3Fref_%3Dadv_li_tt",
                    "ex-hargs=v=1.0;c=IMDB;p=tt1686821;t=imdb_title_view;",
                    "encoding=server",
                    "cb=498172854929"  
                ];
        
            if (document.getElementById('sis_pixel_sitewide')) {
                (document.getElementById('sis_pixel_sitewide')).src = url_sis3 + params_sis3.join('&');
            }
        }
        catch(e){
            if ('consoleLog' in window){
                consoleLog('Pixel failure ' + e.toString(),'sis');
            }
            if (window.ueLogError) { 
                window.ueLogError(e);
            }
        }
    }, 20);
</script>
