                    <button class="btn small" value="Yes" name="ynb_2080123_yes" onclick="CS.TMD.user_review_vote(2080123, 'tt1111422', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2080123_no" onclick="CS.TMD.user_review_vote(2080123, 'tt1111422', 'no');" >No</button>
                </div>
