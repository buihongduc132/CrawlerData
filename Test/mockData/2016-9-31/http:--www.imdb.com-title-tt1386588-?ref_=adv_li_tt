                    <button class="btn small" value="Yes" name="ynb_2298142_yes" onclick="CS.TMD.user_review_vote(2298142, 'tt1386588', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2298142_no" onclick="CS.TMD.user_review_vote(2298142, 'tt1386588', 'no');" >No</button>
                </div>
