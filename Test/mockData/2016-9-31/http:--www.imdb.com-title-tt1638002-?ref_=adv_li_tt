                    <button class="btn small" value="Yes" name="ynb_3502127_yes" onclick="CS.TMD.user_review_vote(3502127, 'tt1638002', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_3502127_no" onclick="CS.TMD.user_review_vote(3502127, 'tt1638002', 'no');" >No</button>
                </div>
