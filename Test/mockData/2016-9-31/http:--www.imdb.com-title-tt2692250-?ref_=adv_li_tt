                    <button class="btn small" value="Yes" name="ynb_3144825_yes" onclick="CS.TMD.user_review_vote(3144825, 'tt2692250', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_3144825_no" onclick="CS.TMD.user_review_vote(3144825, 'tt2692250', 'no');" >No</button>
                </div>
