                    <button class="btn small" value="Yes" name="ynb_3043527_yes" onclick="CS.TMD.user_review_vote(3043527, 'tt1340800', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_3043527_no" onclick="CS.TMD.user_review_vote(3043527, 'tt1340800', 'no');" >No</button>
                </div>
