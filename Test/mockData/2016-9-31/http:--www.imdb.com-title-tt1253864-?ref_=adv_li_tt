                    <button class="btn small" value="Yes" name="ynb_2611531_yes" onclick="CS.TMD.user_review_vote(2611531, 'tt1253864', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2611531_no" onclick="CS.TMD.user_review_vote(2611531, 'tt1253864', 'no');" >No</button>
                </div>
