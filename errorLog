                    <button class="btn small" value="Yes" name="ynb_80747_yes" onclick="CS.TMD.user_review_vote(80747, 'tt0056923', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_80747_no" onclick="CS.TMD.user_review_vote(80747, 'tt0056923', 'no');" >No</button>
                </div>
