                    <button class="btn small" value="Yes" name="ynb_74318_yes" onclick="CS.TMD.user_review_vote(74318, 'tt0054953', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_74318_no" onclick="CS.TMD.user_review_vote(74318, 'tt0054953', 'no');" >No</button>
                </div>
